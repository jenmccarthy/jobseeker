<h2>Company Profile: <span class="company-show-head"><%= @company.name %></span></h2>

<%= link_to "Add Contacts", company_contacts_path(@company), class: "contact-btn btn btn-default", title: "Manage your company_contacts" %>

<h4>Contacts at <%= @company.name %></h4>
<ul>
  <% @company.contacts.each do |contact| %>
	  <li><p><%= link_to "#{contact.first_name} #{contact.last_name} ", company_contact_path(@company, contact) %></p></li>
  <% end %>
</ul>