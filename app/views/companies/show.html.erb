<h4><%= @company.name %></h4>

<%= link_to "Contacts", company_contacts_path(@company), class: "contact-btn btn btn-default", title: "Manage your company_contacts" %>

<ul>
  <% @company.contacts.each do |contact| %>
	  <li><p><%= link_to "#{contact.first_name} #{contact.last_name} ", company_contact_path(@company, contact) %></p></li>
  <% end %>
</ul>